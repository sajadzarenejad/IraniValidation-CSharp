<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageId>IraniValidator.core</PackageId>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Sajad Zarenejad</Authors>
    <Company>3nobar</Company>
    <Product>IraniValidator</Product>
    <Description>Extention methods for Iranian mobile and national Id and some other values validation.</Description>
    <PackageIconUrl>http://3nobar.net/StaticFiles/images/logo/logo2.png</PackageIconUrl>
    <RepositoryType>github</RepositoryType>
    <RepositoryUrl>https://github.com/sajadzarenejad/IraniValidation-CSharp</RepositoryUrl>
    <PackageTags>iranian Mobile validation nationalId</PackageTags>
    <NeutralLanguage>fa-IR</NeutralLanguage>
    <PackageProjectUrl>https://github.com/sajadzarenejad/IraniValidation-CSharp</PackageProjectUrl>
    <PackageLicenseUrl>https://opensource.org/licenses/GPL-3.0</PackageLicenseUrl>
    <Version>1.3.0</Version>
    <PackageReleaseNotes>in this release computer and internet values such as ip address, email and etc added.</PackageReleaseNotes>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <WarningLevel>3</WarningLevel>
  </PropertyGroup>

</Project>
